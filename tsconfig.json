{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "baseUrl": "./src",
    "outDir": "./dist",
    "paths": {
      "ops-frontend/*": [
        "./src/*"
      ],
      "ops-frontend-test/*": [
        "./__tests__/*"
      ]
    },
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.css"
  ]
  // "exclude": ["node_modules"]
  ,
  "exclude": [
    "node_modules"
  ]
}
