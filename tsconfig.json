{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "customConditions": [
      "browser"
    ],
    "isolatedModules": true,
    "incremental": true,
    "baseUrl": "./",
    "outDir": "./dist",
    "paths": {
      // "next/*": [
      //   "./node_modules/next/*"
      // ]
      // "ops-frontend/*": [
      //   "./src/*"
      // ],
      // "ops-frontend-test/*": [
      //   "./__tests__/*"
      // ]
    },
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.css",
    "./src"
  ],
  "exclude": [
    "node_modules",
    "cdk"
  ]
}
