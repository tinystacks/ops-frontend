{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "customConditions": [
      "browser"
    ],
    "isolatedModules": true,
    "incremental": true,
    "baseUrl": "./",
    "outDir": "./dist",
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "types.d.ts",
    "src"
  ],
  "exclude": [
    "node_modules",
    "cdk"
  ]
}
